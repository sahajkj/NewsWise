@{
    ViewData["Title"] = "Seach Result ";
}
<style>
    .SearchResult{
        background-color:#D9D9D9
    }
    .SearchBar1{
        font-family:'Poppins-regular';
        font-size:12px;
    }
    .SearchBut1{
        font-family:'Poppins-semibold';
        font-size:12px;
    }
    .Back5 {
        color: #708090;
        font-size: 10px;
        font-family: 'Roboto-medium';
        position:relative;
        left:50px;
    }

    .Nav5 {
        color: #708090;
        font-size: 10px;
        font-family: 'Roboto-medium';
    }
    .ResultTitle{
        font-family:Poppins-semibold;
        display:flex;
        position:relative;
        left:100px
    }
    .Retle1{
        color:#FFBB00
    }
</style>
<body>
    <div class="SearchTitle bg-black" style="margin-top:-20px;margin-left:-15px;margin-right:-15px">
        <br/>
        <br />
        <br />
        <form runat="server" method="get">
            <div class="searchBody1" align=center>
                <input type="search" class="SearchBar1 p-3 mt-auto text-center" placeholder="SEARCH FOR THE TRUTH..." style="width:50%; background-color:#F5F5F5;border:0px solid;" />
                <input type="submit" runat="server" asp-area="" asp-controller="Home" asp-action="SearchResult" class="SearchBut1 p-3" value="SEARCH" style="background-color: #FFBB00;border:0px solid;">
            </div>
        </form>
        <br/>
        <br />
        <br />
    </div>
    <div class="SearchResult" style="margin-left:-15px;margin-right:-15px">
        <div class="Back5" >
            <p>
                <a class="Nav5" asp-area="" asp-controller="Home" asp-action="Index">Home</a>  >  <a class="Nav5" asp-area="" asp-controller="Home" asp-action="SearchResult">Search Result</a>
            </p>
        </div>
        <div class="ResultTitle">
            <div class="Retle">
            <h1>Search Results</h1>
            </div>
            <div class="Retle1">
                <h1>.</h1>
            </div>
        </div>
    
    </div>
</body>